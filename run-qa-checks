#!/bin/bash

set -e

yarn lint || echo "Try running yarn lint:fix"

openwisp-qa-check --skip-checkmigrations \
                  --skip-checkmakemigrations \
                  --skip-black \
                  --skip-flake8 \
                  --skip-isort \
                  --skip-checkendline \
                  --csslinter \
                  --jslinter
