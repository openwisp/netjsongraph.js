{
  "name": "netjsongraph.js",
  "version": "0.1.3",
  "description": "NetJson format data visualization",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --open --hot --mode development",
    "start": "yarn dev",
    "build": "webpack --progress --hide-modules --mode production",
    "coverage": "jest --coverage",
    "coveralls": "test -f ./coverage/lcov.info && cat ./coverage/lcov.info | coveralls || echo 'no coverage file found'",
    "precommit": "lint-staged",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "format": "prettier --write \"**/*.+(js|json|css|md)\"",
    "format:check": "prettier -c \"**/*.+(js|json|css|md)\""
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/netjson/netjsongraph.js.git",
  "author": "Federico Capoano <federico.capoano@gmail.com> (http://nemesisdesign.net/)",
  "contributors": [
    "GeekPlux <geekplux@gmail.com> (http://geekplux.com/)"
  ],
  "license": "BSD-3-Clause",
  "keywords": [
    "netjson",
    "network",
    "data",
    "visualization"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.4.4",
    "acorn": "^6.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "lint-staged": "^7.2.2",
    "prettier": "^2.6.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "echarts": "^4.2.1",
    "leaflet": "^1.5.1",
    "zrender": "^5.3.2"
  }
}
