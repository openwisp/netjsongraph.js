{
  "name": "netjsongraph.js",
  "version": "0.1.3",
  "description": "NetJson format data visualization",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack serve --open --mode development",
    "start": "yarn dev",
    "build": "webpack --progress --mode production",
    "coverage": "jest --coverage",
    "coveralls": "test -f ./coverage/lcov.info && cat ./coverage/lcov.info | coveralls || echo 'no coverage file found'",
    "precommit": "lint-staged",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "format": "prettier --write \"**/*.+(js|json|css|md)\"",
    "format:check": "prettier -c \"**/*.+(js|json|css|md)\""
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "repository": "https://github.com/netjson/netjsongraph.js.git",
  "author": "Federico Capoano <federico.capoano@gmail.com> (http://nemesisdesign.net/)",
  "contributors": [
    "GeekPlux <geekplux@gmail.com> (http://geekplux.com/)"
  ],
  "license": "BSD-3-Clause",
  "keywords": [
    "netjson",
    "network",
    "data",
    "visualization"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^28.1.6",
    "acorn": "^8.7.1",
    "copy-webpack-plugin": "^11.0.0",
    "coveralls": "^3.1.1",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "echarts": "^5.3.3",
    "echarts-gl": "^2.0.8",
    "kdbush": "^4.0.2",
    "leaflet": "^1.8.0",
    "leaflet.markercluster": "^1.5.3",
    "zrender": "^5.3.2"
  }
}
